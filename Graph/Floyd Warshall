#include<bits/stdc++.h>
 
using namespace std;
#define ll long long
#define fast ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);

const ll M = 1e9 + 7;

int main(){
    fast
    ll n, m; cin >> n >> m;
    vector< vector<ll> > dis(n + 2);
    ll in = 1e18;
    
    for (int i = 1; i <= n; i++) {
        for (int j = 0; j <= n; j++) {
            if (i == j) dis[i].push_back(0);
            else dis[i].push_back(in);
        }
    }
    
    for (int i = 0; i < m; i++) {
        ll a, b, w; cin >> a >> b >> w;
        dis[a][b] = w;
    }

    for (int k = 1; k <= n; k++) {
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= n; j++) {
                dis[i][j] = min (dis[i][j], dis[i][k] + dis[k][j]);
            }
        }
    }

    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            cout << dis[i][j] << " ";
        }
        cout << endl;
    }

}
