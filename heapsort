#include<bits/stdc++.h>

using namespace std;
#define ll long long
const int N=1e3;
void heapify(vector<int>&a,int n,int i){
    int l=2*i+1;
    int r=2*i+2;
    int ma=i;
    if(l<n && a[l]>a[ma]){
        ma=l;
    }
    if(r<n && a[r]>a[ma]){
        ma=r;
    }
    if(ma!=i){
        swap(a[i],a[ma]);
        heapify(a,n,ma);
    }
}
void heapsort(vector<int>&a){
    int n=a.size();
    for(int i=n/2-1;i>=0;i--){
        heapify(a,n,i);
    }
    for(int i=n-1;i>=0;i--){
        swap(a[0],a[i]);
        heapify(a,i,0);
    }
}
int main(){
    int n;
    cin>>n;
    vector<int>a(n);
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    heapsort(a);
    for(auto it: a)cout<<it<<" ";
    cout<<endl;
}
